<?xml version="1.0" encoding="utf-8"?>
<odoo>
   

        <record id="action_quick_sale_report" model="ir.actions.act_window">
        <field name="name">Quick Sale Report</field>
        <field name="res_model">stock.move</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="stock.view_move_tree"/> 
        <field name="domain">[('state', '=', 'done'),('usage','=','internal')]</field> 

       
    </record>

    <record id="menu_quick_sale_view" model="ir.ui.menu">
        <field name="name">Quick Sale Report</field>
        <field name="parent_id" ref="stock.menu_warehouse_report"/>
        <field name="action" ref="action_quick_sale_report"/>
        <field name="sequence" eval="10"/>
    </record>
</odoo>



